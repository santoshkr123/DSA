      "Count digit are : 4\n"
     ]
    }
   ],
   "source": [
    "n=int(input(\"Enter numbers\"))\n",
    "res=0\n",
    "while n>0:\n",
    "    n=n//10\n",
    "    res=res+1\n",
    "\n",
    "print(\"Count digit are :\",res)"
